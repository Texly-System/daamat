{
  "name": "create-damat-app",
  "version": "0.0.1",
  "description": "Create a damat project using a single command.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "create-damat-app": "./dist/index.js"
  },
  "license": "MIT",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "typecheck": "bun tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "@damatjs/deps": "*",
    "boxen": "^8.0.1",
    "cac": "^6.7.14",
    "configstore": "^8.0.0",
    "nanoid": "^4.0.2",
    "node-emoji": "^2.0.2",
    "open": "^9.1.0",
    "picocolors": "^1.1.1",
    "slugify": "^1.6.6",
    "terminal-link": "^5.0.0",
    "wait-on": "^9.0.1",
    "winston": "^3.19.0",
    "yocto-spinner": "^0.1.2"
  },
  "devDependencies": {
    "@types/bun": "1.3.9"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damatjs/damat",
    "directory": "packages/cli/create-damat-app"
  },
  "author": "Abel Lamesgen",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "88279c89e019c471d226216f4bc5be9f9a0c2a4a"
}